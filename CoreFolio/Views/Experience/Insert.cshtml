
@{
    ViewData["Title"] = "Insert";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
    <div class="col-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Yeni Deneyim Ekle</h4>
                <form class="forms-sample" method="post">
                    <div class="form-group">
                        <label for="exampleInputName1">Başlık</label>
                        <input type="text" class="form-control" name="Name" placeholder="Başlık">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail3">Tarih</label>
                    <input type="email" class="form-control" name="Date" placeholder="Tarih">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword4">Görsel URL</label>
                    <input type="text" class="form-control" name="ImageURL" placeholder="Görsel URL">
                    </div>
                <div class="form-group">
                    <label for="exampleInputPassword4">Şirket</label>
                    <input type="text" class="form-control" name="Company" placeholder="Şirket">
                </div>
                    <div class="form-group">
                        <label for="exampleTextarea1">Açıklama</label>
                    <textarea class="form-control" name="Description" rows="4"></textarea>
                    </div>
                    <button type="submit" class="btn btn-primary mr-2">Ekle</button>
                    <a href="/Experience/Index/" class="btn btn-dark">İptal et</a>
                </form>
            </div>
        </div>
    </div>

